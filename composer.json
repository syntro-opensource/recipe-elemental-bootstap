{
    "name": "syntro/recipe-elemental-bootstrap",
    "description": "Recipe to install basic bootstrap components",
    "type": "silverstripe-recipe",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Matthias Leutenegger",
            "email": "hello@syntro.ch"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "silverstripe/recipe-plugin": "^1",
        "syntro/silverstripe-elemental-bootstrap-baseitems": "~0.3",
        "syntro/silverstripe-elemental-bootstrap-accordionsection": "0.3.0",
        "syntro/silverstripe-elemental-bootstrap-alertsection": "0.3.0",
        "syntro/silverstripe-elemental-bootstrap-featuresection": "0.3.0",
        "syntro/silverstripe-elemental-bootstrap-gallerysection": "0.3.0",
        "syntro/silverstripe-elemental-bootstrap-listgroupsection": "0.3.0",
        "syntro/silverstripe-elemental-bootstrap-spotlightsection": "0.3.0",
        "syntro/silverstripe-elemental-bootstrap-staffsection": "0.3.0",
        "syntro/silverstripe-elemental-bootstrap-testimonialsection": "0.3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "sminnee/phpunit-mock-objects": "^3.4.5"
    },
    "extra": {
        "project-files": [
            "app/_config/elemental.yml",
            "app/templates/*"
        ]
    }
}
